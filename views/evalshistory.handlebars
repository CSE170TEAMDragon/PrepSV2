<!doctype html>

<html>
<head>
	<title>Evals History</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="css/bootstrap.min.css" rel="stylesheet">
	<link href="css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="css/evalshistory.css" rel="stylesheet">
	<link href="css/levelbtn.css" rel="stylesheet">

	<link rel="stylesheet" href="/css/hamburgerbar.css">
 
</head>

<body>

	<header>
  			<button class="hamburger">&#9776;</button>
  			<button class="cross">&#735;</button>
  			<p class="myButton">{{levelName}}</p>
	</header>

	<div class="menu">
 	 <ul>
    	<a href="/question"><li>Questions</li></a>
    	<a href="/achievement"><li>Achievement</li></a>
    	<a href="/learnmore"><li>Learn More</li></a>
    	<a href="/"><li>Log out</li></a>
  	</ul>
	</div>

	<div class="container">
		<!-- h1 through h6 are headers. The higher the number, the smaller the header -->
		<!-- p means paragraph -->
		<div class="jumbotron">
			<h1 style="color:white">Response History</h1>
			<h4 style="color:white; padding-left: 10px; padding-right:10px">For the question: {{question}}</h4>
		</div>

		{{> popup}}


		{{#each data}}
		<div class="panel panel-info" style="margin:4% 8% 0% 8%">
		  <div class="panel-heading" >
		    <h2 class="panel-title" style="text-align:center; font-size: 26px" >{{position}} </h2>
		  </div>
		  <div class="panel-body" id="historyList">
		        	<h4>Performed <b>Great</b> on:</h4>
		        	<ul>
		        		<li> {{cItem1}} </li>
		        		<li> {{cItem2}} </li>
		        	</ul>
		        	<h4>Ways to Improve:</h4>
		        	<ul>
		        		<li> {{uItem1}} </li>
		        		<li> {{uItem2}} </li>
		        	</ul>

		  </div>
		</div>
		{{/each}}


	</div>

  
	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="js/bootstrap.min.js"></script>
	<script src="js/introHCI.js"></script>
</body>

</html>
